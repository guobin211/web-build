{
  "name": "@web-build/esbuild",
  "version": "0.0.0",
  "description": "esbuild tool",
  "author": "GuoBin <michaelbguo@tencent.com>",
  "homepage": "https://github.com/guobin211/web-build#readme",
  "license": "ISC",
  "directories": {
    "test": "__tests__"
  },
  "files": [
    "build"
  ],
  "type": "module",
  "main": "./build/index.js",
  "module": "./build/index.js",
  "exports": {
    ".": {
      "import": "./build/index.js",
      "require": "./build/index.js"
    }
  },
  "publishConfig": {
    "registry": "https://www.npmjs.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guobin211/web-build.git"
  },
  "scripts": {
    "build": "esbuild src/index.ts --bundle --format=esm --target=es6 --outdir=build --sourcemap",
    "start": "esbuild src/index.ts --watch --outdir=build --sourcemap"
  },
  "bugs": {
    "url": "https://github.com/guobin211/web-build/issues"
  },
  "devDependencies": {
    "esbuild": "^0.11.16"
  }
}
