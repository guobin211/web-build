{
  "name": "@web-build/babel",
  "version": "0.0.0",
  "description": "about babel",
  "keywords": [
    "babel",
    "doc"
  ],
  "author": "GuoBin <michaelbguo@tencent.com>",
  "homepage": "https://github.com/guobin211/web-build#readme",
  "license": "ISC",
  "directories": {
    "test": "__tests__"
  },
  "files": [
    "build"
  ],
  "type": "module",
  "main": "./build/index.umd.js",
  "module": "./build/index.es.js",
  "exports": {
    ".": {
      "import": "./build/index.es.js",
      "require": "./build/index.umd.js"
    }
  },
  "publishConfig": {
    "registry": "https://www.npmjs.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guobin211/web-build.git"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "build:prod": "babel --config-file ./babel.prod.json --out-file dist/index.js ./src/*.ts"
  },
  "bugs": {
    "url": "https://github.com/guobin211/web-build/issues"
  },
  "devDependencies": {
    "core-js": "^3.13.1",
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/runtime": "^7.14.0",
    "@babel/plugin-transform-runtime": "^7.13.15"
  }
}
