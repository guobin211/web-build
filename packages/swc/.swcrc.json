[
  {
    "test": ".*.ts$",
    "module": {
      "type": "umd",
      "strict": false,
      "strictMode": true,
      "lazy": false,
      "noInterop": false
    },
    "env": {
      "targets": {
        "chrome": "79",
        "ie": "11"
      },
      "mode": "entry",
      "coreJs": 3,
      "dynamicImport": false
    },
    "jsc": {
      "parser": {
        "syntax": "typescript",
        "tsx": false,
        "decorators": true,
        "dynamicImport": true
      }
    },
    "minify": false,
    "sourceMaps": true
  },
  {
    "test": ".*.js$",
    "module": {
      "type": "commonjs"
    },
    "minify": false,
    "sourceMaps": true,
    "env": {
      "targets": {
        "chrome": "79",
        "ie": "11"
      },
      "mode": "entry",
      "coreJs": 3,
      "dynamicImport": false
    }
  }
]
