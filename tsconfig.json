{
  "compilerOptions": {
    "jsx": "react",
    "moduleResolution": "node",
    "isolatedModules": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "lib": ["DOM", "ESNext", "WebWorker"],
    "target": "ESNext",
    "module": "ESNext",
    "allowJs": true,
    "rootDir": ".",
    "baseUrl": ".",
    "outDir": "build"
  }
}
